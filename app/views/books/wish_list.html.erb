<div class="container container-margin-left">

  <%= link_to books_path do %>
    <i class="fas fa-arrow-alt-circle-left icon-back"></i>
  <% end %>

  <h1 class="bg-big bg-blue el-center mb-5 el-center">My WishList : </h1>

  <% @wish_list_books.each do |book| %>
    <div class="container-book">
    <%= link_to book.name , book_path(book), class:"txt-book" %>
    <!-- TODO add btn to add book in reading list-->
    </div>
  <% end %>
</div>
